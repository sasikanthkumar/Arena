<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme" parent="AppTheme.Base">
        <item name="android:colorPrimary">@color/primaryColor</item>
        <item name="android:colorPrimaryDark">@color/primaryColorDark</item>
        <item name="android:colorAccent">@color/accentColor</item>
        <item name="android:colorButtonNormal">#2196f3</item>
        <item name="android:windowActionBarOverlay">true</item>


    </style>

    <style name="ParallaxTheme" parent="ParallaxTheme.Base">
        <item name="android:colorPrimary">@android:color/transparent</item>
        <item name="android:colorPrimaryDark">@color/primaryColorDarkParallax</item>
        <item name="android:colorAccent">@color/accentColor</item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:hideOnContentScroll">true</item>
    </style>

    <style name="MovingToolbarTheme" parent="MainTheme">


    </style>




</resources>