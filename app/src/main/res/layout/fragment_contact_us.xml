<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="org.arenatest.bits.arena_test.ContactUs">

   <ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:ripple="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:background="@drawable/sports2"
        android:fillViewport="true"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/LinearLayout01"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <com.balysv.materialripple.MaterialRippleLayout
                android:layout_width="match_parent"
                app:rippleColor="@color/primaryColorDark"
                app:rippleAlpha="1"

                android:layout_height="wrap_content">


                <LinearLayout
                    android:id="@+id/harsha1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:clickable="true"
                    android:padding="10dp">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/view"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:src="@drawable/harsha"
                        app:border_width="2dp" />

                    <LinearLayout
                        android:id="@+id/linearLayout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:gravity="center"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:alpha="0.87"
                            android:text="Harsha Aechuri"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="#000000" />

                        <TextView
                            android:id="@+id/textView2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:alpha="0.54"
                            android:text="Sports Secretary,Students Union"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:textColor="#000000"
                            android:textSize="13sp" />

                    </LinearLayout>


                </LinearLayout>
            </com.balysv.materialripple.MaterialRippleLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_marginLeft="110dp"
                android:layout_marginRight="10dp"
                android:background="#E6E6E6" />

            <com.balysv.materialripple.MaterialRippleLayout
                android:layout_width="match_parent"
                app:rippleColor="@color/primaryColorDark"
                app:rippleAlpha="1"

                android:layout_height="wrap_content">

                <LinearLayout
                    android:id="@+id/battar2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:clickable="true"
                    android:padding="10dp">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/view2"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:src="@drawable/battar"
                        app:border_width="2dp" />

                    <LinearLayout
                        android:id="@+id/linearLayout2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:gravity="center"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textView3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:alpha="0.87"
                            android:text="Shashank Battar"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="#000000" />

                        <TextView
                            android:id="@+id/textView4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:alpha="0.54"
                            android:text="Registration and Hospitatilty"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:textColor="#000000" />

                    </LinearLayout>


                </LinearLayout>
            </com.balysv.materialripple.MaterialRippleLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_marginLeft="110dp"
                android:layout_marginRight="10dp"
                android:background="#E6E6E6" />

            <com.balysv.materialripple.MaterialRippleLayout
                android:layout_width="match_parent"
                app:rippleColor="@color/primaryColorDark"
                app:rippleAlpha="1"

                android:layout_height="wrap_content">


                <LinearLayout
                    android:id="@+id/mrudul3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:clickable="true"
                    android:orientation="horizontal"
                    android:padding="10dp">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/view3"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:src="@drawable/mrudul"
                        app:border_width="2dp" />

                    <LinearLayout
                        android:id="@+id/linearLayout3"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_centerHorizontal="true"
                        android:gravity="center"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textView5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:alpha="0.87"
                            android:text="Mrudul Nekkanti"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="#000000" />

                        <TextView
                            android:id="@+id/textView6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:alpha="0.54"
                            android:text="Sponsorship and Marketing"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:textColor="#000000" />

                    </LinearLayout>


                </LinearLayout>
            </com.balysv.materialripple.MaterialRippleLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_marginLeft="110dp"
                android:layout_marginRight="10dp"
                android:background="#E6E6E6" />

            <com.balysv.materialripple.MaterialRippleLayout
                android:layout_width="match_parent"
                app:rippleAlpha="1"
                app:rippleColor="@color/primaryColorDark"

                android:layout_height="wrap_content">

                <LinearLayout
                    android:id="@+id/snehith4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:clickable="true"
                    android:padding="10dp">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/view4"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:src="@drawable/snehith"
                        app:border_width="2dp" />

                    <LinearLayout
                        android:id="@+id/linearLayout4"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:gravity="center"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textView7"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:alpha="0.87"
                            android:text="Snehith Budime"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="#000000" />

                        <TextView
                            android:id="@+id/textView8"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:alpha="0.54"
                            android:text="Pubilcity and Public Relations"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:textColor="#000000" />

                    </LinearLayout>


                </LinearLayout>
            </com.balysv.materialripple.MaterialRippleLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_marginLeft="110dp"
                android:layout_marginRight="10dp"
                android:background="#E6E6E6" />

            <com.balysv.materialripple.MaterialRippleLayout
                app:rippleColor="@color/primaryColorDark"
                app:rippleAlpha="1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:id="@+id/srivani5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:clickable="true"
                    android:padding="10dp">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/view5"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:src="@drawable/srivani"
                        app:border_width="2dp" />

                    <LinearLayout
                        android:id="@+id/linearLayout5"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:gravity="center"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textView9"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:alpha="0.87"
                            android:text="Shrivani Pandiya"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="#000000" />

                        <TextView
                            android:id="@+id/textView10"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:alpha="0.54"
                            android:text="Sports Secretary, Students Union "
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:textColor="#000000"
                            android:textSize="13sp" />

                    </LinearLayout>


                </LinearLayout>
            </com.balysv.materialripple.MaterialRippleLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_marginLeft="110dp"
                android:layout_marginRight="10dp"
                android:background="#E6E6E6" />
        </LinearLayout>

    </ScrollView>


</FrameLayout>